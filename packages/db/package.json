{
  "name": "@aniways/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./env.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf .turbo node_modules dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "push": "bun with-env drizzle-kit push",
    "studio": "bun with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@aniways/web-scraping": "workspace:*",
    "@paralleldrive/cuid2": "^2.2.2",
    "@t3-oss/env-nextjs": "^0.10.1",
    "drizzle-orm": "^0.31.2",
    "lodash": "^4.17.21",
    "pg": "^8.12.0",
    "postgres": "^3.4.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aniways/eslint-config": "workspace:*",
    "@aniways/prettier-config": "workspace:*",
    "@aniways/typescript-config": "workspace:*",
    "eslint": "^9.4.0",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5",
    "@types/lodash": "^4.14.202",
    "drizzle-kit": "^0.22.7",
    "dotenv-cli": "^7.4.2"
  },
  "prettier": "@aniways/prettier-config"
}
