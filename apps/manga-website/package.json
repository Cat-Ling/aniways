{
  "name": "@aniways/manga-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun with-env vite dev",
    "build": "bun with-env sst build --stage prod",
    "vite:build": "tsc && vite build",
    "aws:dev": "bun with-env sst bind vite --stage dev",
    "clean": "rm -rf .turbo node_modules .sst dist",
    "format": "prettier --check . --ignore-path ../../.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "deploy:prod": "bun sst deploy --stage prod",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@aniways/db": "workspace:*",
    "@aniways/myanimelist": "workspace:*",
    "@aniways/trpc": "workspace:*",
    "@aniways/ui": "workspace:*",
    "@tanstack/react-query": "^5.21.7",
    "@tanstack/react-router": "^1.45.8",
    "@trpc/client": "11.0.0-rc.364",
    "framer-motion": "^11.3.17",
    "lucide-react": "^0.316.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "superjson": "2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aniways/eslint-config": "workspace:*",
    "@aniways/prettier-config": "workspace:*",
    "@aniways/typescript-config": "workspace:*",
    "@tanstack/router-devtools": "^1.45.8",
    "@tanstack/router-plugin": "^1.45.8",
    "@types/aws-lambda": "^8.10.140",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.4.0",
    "jiti": "^1.21.0",
    "prettier": "^3.3.1",
    "sst": "2.43.1",
    "typescript": "^5.4.5",
    "vite": "^5.3.1"
  },
  "prettier": "@aniways/prettier-config"
}
