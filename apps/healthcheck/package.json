{
  "name": "@aniways/healthcheck",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bunx with-env sst build && bun with-env tsc && bun with-env vite build",
    "dev": "bunx with-env sst bind vite --stage dev",
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "deploy:prod": "bunx with-env sst deploy --stage prod",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@aniways/data": "workspace:*",
    "@aniways/db": "workspace:*",
    "@aniways/myanimelist": "workspace:*",
    "@aniways/ui": "workspace:*",
    "@tanstack/react-query": "^5.45.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@aniways/eslint-config": "workspace:*",
    "@aniways/prettier-config": "workspace:*",
    "@aniways/typescript-config": "workspace:*",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.4.0",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5",
    "aws-cdk-lib": "2.142.1",
    "constructs": "10.3.0",
    "sst": "^2.43.1",
    "vite": "^5.3.1"
  },
  "prettier": "@aniways/prettier-config"
}
