{
  "name": "@aniways/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun with-env sst build --stage prod",
    "aws:dev": "bun with-env sst dev --stage dev",
    "clean": "rm -rf .turbo node_modules .sst",
    "format": "prettier --check . --ignore-path ../../.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "deploy:prod": "bun sst deploy --stage prod",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@aniways/auth": "workspace:*",
    "@aniways/trpc": "workspace:*",
    "@trpc/server": "11.0.0-rc.364"
  },
  "devDependencies": {
    "@aniways/eslint-config": "workspace:*",
    "@aniways/prettier-config": "workspace:*",
    "@aniways/typescript-config": "workspace:*",
    "@types/aws-lambda": "^8.10.140",
    "aws-cdk-lib": "2.142.1",
    "constructs": "10.3.0",
    "eslint": "^9.4.0",
    "jiti": "^1.21.0",
    "prettier": "^3.3.1",
    "sst": "^2.43.1",
    "typescript": "^5.4.5"
  },
  "prettier": "@aniways/prettier-config"
}
