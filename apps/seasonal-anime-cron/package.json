{
  "name": "@aniways/seasonal-anime-cron",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun with-env sst build --stage prod",
    "aws:dev": "bun with-env sst dev --stage dev",
    "clean": "rm -rf .turbo node_modules .sst",
    "format": "prettier --check . --ignore-path ../../.prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "deploy:prod": "bun sst deploy --stage prod",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@aniways/trpc": "workspace:*",
    "@aniways/db": "workspace:*",
    "@aniways/myanimelist": "workspace:*",
    "@trpc/client": "11.0.0-rc.364",
    "superjson": "2.2.1"
  },
  "devDependencies": {
    "@aniways/eslint-config": "workspace:*",
    "@aniways/prettier-config": "workspace:*",
    "@aniways/typescript-config": "workspace:*",
    "eslint": "9.4.0",
    "jiti": "1.21.0",
    "prettier": "3.3.1",
    "typescript": "5.4.5",
    "aws-cdk-lib": "2.142.1",
    "constructs": "10.3.0",
    "sst": "2.43.1"
  },
  "prettier": "@aniways/prettier-config"
}
